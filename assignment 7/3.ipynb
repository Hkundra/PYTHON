    "# initialize the dictionaries\n",
    "dic1 = {1: 10, 2: 20}\n",
    "dic2 = {3: 30, 4: 40}\n",
    "dic3 = {5: 50, 6: 60}\n",
    "\n",
    "# concatenate the dictionaries\n",
    "new_dict = {}\n",
    "for d in [dic1, dic2, dic3]:\n",
