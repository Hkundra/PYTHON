    "def generate_dict(n):\n",
    "    my_dict = {}\n",
    "    for x in range(1, n+1):\n",
    "        my_dict[x] = x*x\n",
    "    return my_dict\n",
    "\n",
    "\n",
    "my_dict = generate_dict(5)\n",
    "print(my_dict)\n"
